{
  "name": "meson-contracts-solidity",
  "version": "1.0.0-alpha.1",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": "16"
  },
  "scripts": {
    "build": "hardhat compile --config hardhat.config.ts",
    "postbuild": "cp artifacts/contracts/Meson.sol/Meson.json packages/contract-abis/abis/Meson.json && cp artifacts/contracts/test/MockToken.sol/MockToken.json packages/contract-abis/abis/ERC20.json",
    "cleanbuild": "hardhat clean && yarn build",
    "build:packages": "yarn workspace @mesonfi/sdk build && yarn workspace @mesonfi/presets build",
    "build:types": "TESTNET_MODE=true yarn chain:eth && yarn cleanbuild",
    "test": "TESTNET_MODE=true NETWORK_ID=eth yarn chain && hardhat --network hardhat test",
    "docgen": "solidity-docgen --solc-module solc -t ./templates",
    "estimate": "hardhat --network hardhat run scripts/estimate-gas.js",
    "estimate-upgradable": "hardhat --network hardhat run scripts/estimate-gas-upgradable.js",
    "chain": "node scripts/set-chain-config.js",
    "chain:local": "NETWORK_ID=local yarn chain",
    "chain:eth": "NETWORK_ID=eth yarn chain",
    "chain:bnb": "NETWORK_ID=bnb yarn chain",
    "chain:avax": "NETWORK_ID=avax yarn chain",
    "chain:poly": "NETWORK_ID=polygon yarn chain",
    "chain:ftm": "NETWORK_ID=ftm yarn chain",
    "chain:cfx": "NETWORK_ID=cfx yarn chain",
    "chain:one": "NETWORK_ID=one yarn chain",
    "chain:aurora": "NETWORK_ID=aurora yarn chain",
    "chain:evmos": "NETWORK_ID=evmos && yarn chain",
    "chain:arbitrum": "NETWORK_ID=arbitrum yarn chain",
    "chain:optimistic": "NETWORK_ID=optimistic yarn chain",
    "chain:gnosis": "NETWORK_ID=gnosis yarn chain",
    "chain:tron": "NETWORK_ID=tron yarn chain",
    "testnet": "TESTNET_MODE=true yarn chain && hardhat run scripts/testnets/deploy-upgradable.js",
    "testnet:eth": "NETWORK_ID=eth yarn testnet",
    "testnet:bnb": "NETWORK_ID=bnb yarn testnet",
    "testnet:poly": "NETWORK_ID=polygon yarn testnet",
    "testnet:avax": "NETWORK_ID=avax yarn testnet",
    "testnet:ftm": "NETWORK_ID=ftm yarn testnet",
    "testnet:cfx": "NETWORK_ID=cfx yarn testnet",
    "testnet:one": "NETWORK_ID=one yarn testnet",
    "testnet:aurora": "NETWORK_ID=aurora yarn testnet",
    "testnet:evmos": "NETWORK_ID=evmos yarn testnet",
    "testnet:gnosis": "NETWORK_ID=gnosis yarn testnet",
    "testnet:tron": "NETWORK_ID=tron TESTNET_MODE=true yarn chain && hardhat run scripts/tron/deploy.js",
    "testnet-up": "TESTNET_MODE=true yarn chain && hardhat run scripts/testnets/upgrade.js",
    "testnet-up:eth": "NETWORK_ID=eth yarn testnet-up",
    "testnet-up:bnb": "NETWORK_ID=bnb yarn testnet-up",
    "testnet-up:poly": "NETWORK_ID=polygon yarn testnet-up",
    "testnet-up:avax": "NETWORK_ID=avax yarn testnet-up",
    "testnet-up:ftm": "NETWORK_ID=ftm yarn testnet-up",
    "testnet-up:cfx": "NETWORK_ID=cfx yarn testnet-up",
    "testnet-up:one": "NETWORK_ID=one yarn testnet-up",
    "testnet-up:aurora": "NETWORK_ID=aurora yarn testnet-up",
    "testnet-up:evmos": "NETWORK_ID=evmos yarn testnet-up",
    "testnet-up:gnosis": "NETWORK_ID=gnosis yarn testnet-up",
    "testnet-up:tron": "NETWORK_ID=tron TESTNET_MODE=true yarn chain && hardhat run scripts/tron/update.js",
    "deploy": "yarn chain && hardhat run scripts/mainnets/deploy-upgradable.js",
    "deploy:eth": "HARDHAT_NETWORK=eth yarn deploy",
    "deploy:bnb": "HARDHAT_NETWORK=bnb yarn deploy",
    "deploy:poly": "HARDHAT_NETWORK=polygon yarn deploy",
    "deploy:avax": "HARDHAT_NETWORK=avax yarn deploy",
    "deploy:ftm": "HARDHAT_NETWORK=ftm yarn deploy",
    "deploy:arb": "HARDHAT_NETWORK=arb yarn deploy",
    "deploy:opt": "HARDHAT_NETWORK=opt yarn deploy",
    "deploy:one": "HARDHAT_NETWORK=one yarn deploy",
    "deploy:aurora": "HARDHAT_NETWORK=aurora yarn deploy",
    "deploy:cfx": "HARDHAT_NETWORK=cfx yarn deploy",
    "deploy:evmos": "HARDHAT_NETWORK=evmos yarn deploy",
    "deploy:movr": "HARDHAT_NETWORK=movr yarn deploy",
    "deploy:beam": "HARDHAT_NETWORK=beam yarn deploy",
    "deploy:tron": "NETWORK_ID=tron yarn chain && hardhat run scripts/tron/deploy.js",
    "up": "yarn chain && hardhat run scripts/mainnets/upgrade.js",
    "up:eth": "HARDHAT_NETWORK=eth yarn up",
    "up:bnb-18": "HARDHAT_NETWORK=bnb yarn up",
    "up:poly": "HARDHAT_NETWORK=polygon yarn up",
    "up:avax": "HARDHAT_NETWORK=avax yarn up",
    "up:ftm": "HARDHAT_NETWORK=ftm yarn up",
    "up:arb": "HARDHAT_NETWORK=arb yarn up",
    "up:opt": "HARDHAT_NETWORK=opt yarn up",
    "up:one": "HARDHAT_NETWORK=one yarn up",
    "up:cfx-18": "HARDHAT_NETWORK=cfx yarn up",
    "up:aurora": "HARDHAT_NETWORK=aurora yarn up",
    "up:evmos": "HARDHAT_NETWORK=evmos yarn up",
    "up:movr": "HARDHAT_NETWORK=movr yarn up",
    "up:beam": "HARDHAT_NETWORK=beam yarn up",
    "up:tron": "HARDHAT_NETWORK=tron yarn chain && hardhat run scripts/tron/update.js",
    "pool": "hardhat run scripts/mainnets/pool.js",
    "pool:eth": "HARDHAT_NETWORK=eth yarn pool",
    "pool:bnb": "HARDHAT_NETWORK=bnb yarn pool",
    "pool:poly": "HARDHAT_NETWORK=polygon yarn pool",
    "pool:avax": "HARDHAT_NETWORK=avax yarn pool",
    "pool:ftm": "HARDHAT_NETWORK=ftm yarn pool",
    "pool:arb": "HARDHAT_NETWORK=arb yarn pool",
    "pool:opt": "HARDHAT_NETWORK=opt yarn pool",
    "pool:one": "HARDHAT_NETWORK=one yarn pool",
    "pool:aurora": "HARDHAT_NETWORK=aurora yarn pool",
    "pool:cfx": "HARDHAT_NETWORK=cfx yarn pool",
    "pool:evmos": "HARDHAT_NETWORK=evmos yarn pool",
    "pool:movr": "HARDHAT_NETWORK=movr yarn pool",
    "pool:beam": "HARDHAT_NETWORK=beam yarn pool",
    "pool:tron": "hardhat run scripts/tron/pool.js",
    "token": "yarn chain && hardhat run scripts/mainnets/token.js",
    "token:avax": "HARDHAT_NETWORK=avax yarn token",
    "token:arb": "HARDHAT_NETWORK=arb yarn token",
    "token:cfx": "HARDHAT_NETWORK=cfx yarn token",
    "token:evmos": "HARDHAT_NETWORK=evmos yarn token"
  },
  "devDependencies": {
    "@mesonfi/contract-abis": "^1.0.0-alpha.1",
    "@mesonfi/contract-types": "^1.0.0-alpha.1",
    "@mesonfi/presets": "^1.0.0-alpha.1",
    "@mesonfi/sdk": "^1.0.0-alpha.1",
    "@nomiclabs/hardhat-ethers": "^2.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.7.2",
    "@openzeppelin/contracts-upgradeable": "^4.7.2",
    "@openzeppelin/hardhat-upgrades": "^1.20.0",
    "@swc/core": "^1.2.224",
    "@swc/helpers": "^0.4.3",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "chai": "^4.3.6",
    "dotenv": "^16.0.1",
    "ethereum-waffle": "^4.0.4",
    "ethers": "^5.6.1",
    "hardhat": "2.10.1",
    "hardhat-change-network": "^0.0.7",
    "mocha": "^10.0.0",
    "mocha-chai-jest-snapshot": "^1.1.4",
    "regenerator-runtime": "^0.13.9",
    "solc": "0.8.6",
    "solidity-docgen": "^0.5.16",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.7.4"
  },
  "workspaces": [
    "packages/*"
  ]
}
