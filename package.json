    "testnet:avax": "hardhat deploy --testnet avax --upgradable true",
    "testnet:tron": "hardhat deploy --testnet tron --upgradable true",
    "testnet:linea": "hardhat deploy --testnet linea --upgradable true",
    "testnet:scroll": "hardhat deploy --testnet scroll --upgradable true",
